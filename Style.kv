<RoundedButton@Button>:
    background_color: (0,0,0,0)
    background_normal: ""
    background_active: ""
    backgroundColorNormal: (0,0,1,1)
    backgroundColorDown: (1,0,0,1)
    border_radius: [18]
    canvas.before:
        Color:
            rgba: self.backgroundColorNormal if self.state == "normal" else self.backgroundColorDown
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: self.border_radius