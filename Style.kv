                Button:
                    size_hint: 1, .08
                    text: "Next Hole"
                    background_normal: ""
                    background_color: app.hexToKivyColor("#008ae6",1)
                    on_release:
                        if root.hole == 0: root.startGame();
                        elif root.hole != 18: root.updateHole();
                        else:\
                        root.updateData();\
                        root.updateEndScreen();\
                        app.root.transition = CardTransition(direction = "down", mode = "push");\
                        app.root.current = "EndScreen";\

